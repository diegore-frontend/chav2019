							include ../partials/part.post.pug
							include ../partials/part.post.pug
							include ../partials/part.post.pug

					#ap-tab-02.ap-tab-content.ap-tab--secnd
						.ap-tab-flx
							include ../partials/part.post-oficial-01.pug
							include ../partials/part.post-oficial-02.pug
							include ../partials/part.post-oficial-03.pug
							include ../partials/part.post-oficial-04.pug
							include ../partials/part.post-oficial-05.pug
							include ../partials/part.post-oficial-06.pug

					#ap-tab-03.ap-tab-content.ap-tab--third
						.ap-tab-flx
							include ../partials/part.post.pug
							include ../partials/part.post.pug
							include ../partials/part.post.pug